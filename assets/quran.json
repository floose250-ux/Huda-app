{
  "surahs": [
    { "id": 1, "name": "الفاتحة", "verses": [{
  "id": 1,
  "name": "الفاتحة",
  "verses": [
    {
      "id": 1,
      "text": "بِسْمِ اللَّهِ الرَّحْمَنِ الرَّحِيمِ",
      "tafsir": "أبدأ قراءتي مستعيناً بالله المعبود بحق، واسع الرحمة.",
      "irab": "الباء: حرف جر، اسم: اسم مجرور، الله: لفظ الجلالة مضاف إليه."
    },
    {
      "id": 2,
      "text": "الْحَمْدُ لِلَّهِ رَبِّ الْعَالَمِينَ",
      "tafsir": "الثناء والحمد لله وحده، خالق كل شيء ومالكه ومدبره.",
      "irab": "الحمد: مبتدأ مرفوع، لله: جار ومجرور متعلقان بمحذوف خبر."
    },
    {
      "id": 3,
      "text": "الرَّحْمَنِ الرَّحِيمِ",
      "tafsir": "صاحب الرحمة الواسعة التي وسعت كل شيء.",
      "irab": "نعت للفظ الجلالة (الله) مجرور."
    },
    {
      "id": 4,
      "text": "مَالِكِ يَوْمِ الدِّينِ",
      "tafsir": "هو وحده سبحانه المتصرف في يوم الجزاء والحساب.",
      "irab": "مالك: نعت ثالث، يوم: مضاف إليه، الدين: مضاف إليه ثانٍ."
    },
    {
      "id": 5,
      "text": "إِيَّاكَ نَعْبُدُ وَإِيَّاكَ نَسْتَعِينُ",
      "tafsir": "نخصك وحدك بالعبادة، ونطلب منك وحدك العون.",
      "irab": "إياك: ضمير منفصل مبني في محل نصب مفعول به مقدم."
    },
    {
      "id": 6,
      "text": "اهْدِنَا الصِّرَاطَ الْمُسْتَقِيمَ",
      "tafsir": "وفقنا وأرشدنا وثبتنا على الطريق الموصل للجنة.",
      "irab": "اهدنا: فعل أمر (للدعاء)، نا: مفعول به، الصراط: مفعول به ثانٍ."
    },
    {
      "id": 7,
      "text": "صِرَاطَ الَّذِينَ أَنْعَمْتَ عَلَيْهِمْ غَيْرِ الْمَغْضُوبِ عَلَيْهِمْ وَلَا الضَّالِّينَ",
      "tafsir": "طريق الذين هديتهم من الأنبياء، لا طريق المغضوب عليهم ولا الضالين.",
      "irab": "صراط: بدل من الأول، الذين: اسم موصول مضاف إليه."
    }
  ]
}
] },
