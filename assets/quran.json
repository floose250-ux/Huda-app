{
  "1": {
    "name": "الفاتحة",
    "verses": [
      {
        "id": 1,
        "text": "بِسْمِ اللَّهِ الرَّحْمَنِ الرَّحِيمِ",
        "tafsir": "نبدأ مستعينين بالله وحده..",
        "irab": "الباء حرف جر، واسم مجرور.."
      },
      {
        "id": 2,
        "text": "الْحَمْدُ لِلَّهِ رَبِّ الْعَالَمِينَ",
        "tafsir": "الثناء والحمد لله خالق كل شيء..",
        "irab": "الحمدُ مبتدأ مرفوع.."
      }
    ]
  }
}
